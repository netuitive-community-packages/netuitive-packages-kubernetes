{
  "analyticConfiguration" : {
    "tenantId" : null,
    "type" : "COMPUTED_METRIC",
    "id" : null,
    "packageId" : null,
    "name" : "Kubernetes Cluster",
    "scope" : {
      "elementName" : null,
      "elementType" : "Kubernetes Cluster",
      "elementTags" : null,
      "metricMatches" : null
    },
    "metrics" : [ {
      "match" : "k8s.cluster.cpu.usage.percent",
      "properties" : {
        "expression" : "data['cpu.request'].actual == 0 ? 0 : 100 * (data['cpu.usage_rate'].actual / data['cpu.request'].actual)",
        "fqn" : "k8s.cluster.cpu.usage.percent",
        "name" : "Cluster CPU Usage Percent"
      }
    }, {
      "match" : "k8s.cluster.memory.usage.percent",
      "properties" : {
        "expression" : "data['memory.request'].actual == 0 ? 0 : 100 * (data['memory.usage'].actual / data['memory.request'].actual)",
        "fqn" : "k8s.cluster.memory.usage.percent",
        "name" : "Cluster Memory Usage Percent"
      }
    } ]
  }
}
