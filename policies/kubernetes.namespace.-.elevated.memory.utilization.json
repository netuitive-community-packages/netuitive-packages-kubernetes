{
  "policy": {
    "actions": [
      {
        "category": 2,
        "type": "event"
      }
    ],
    "conditions": [
      {
        "analytic": "baselineDeviation",
        "metric": "memory.usage.percent",
        "operator": ">"
      },
      {
        "analytic": "actual",
        "level": 90,
        "metric": "memory.usage.percent",
        "operator": ">"
      }
    ],
    "deleted": false,
    "description": "This namespace has a higher than normal memory utilization.",
    "duration": 600,
    "enabled": true,
    "name": "Kubernetes Namespace - Elevated Memory Utilization",
    "scope": {
      "elementAttributesAll": true,
      "elementTagsAll": true,
      "elementTypes": [
        "Kubernetes Namespace"
      ]
    }
  }
}